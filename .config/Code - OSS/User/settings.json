{
    "python.venvFolders": [
        "code"
    ],
    "files.exclude": {
        "**/__pycache__": true,
        "**/.gitignore": true,
        "**/.ipynb_checkpoints": true,
        "**/.vscode": true,
        "**/_DS_Store": true
    },
    "editor.cursorBlinking": "solid",
    "workbench.enableExperiments": false,
    "workbench.tips.enabled": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "terminal.external.linuxExec": "termite",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.editor.enablePreview": false,
    "python.dataScience.askForKernelRestart": false,
    "python.venvPath": "code",
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "explorer.confirmDragAndDrop": false,
    "editor.minimap.enabled": false,
    "git.enableSmartCommit": true,
    "explorer.confirmDelete": false,
    "editor.renderWhitespace": "none",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontSize": 13,
    "[python]": {},
    "git.confirmSync": false,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.dataScience.notebookFileRoot": "${workspaceFolder}",
    "vim.cursorStylePerMode.insert": "line",
    "vim.hlsearch": true,
    "vim.ignorecase": false,
    "vim.leader": " ",
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": true,
    "latex-workshop.bibtex-format.tab": "4 spaces",
    "files.trimTrailingWhitespace": true,
    "vim.searchHighlightTextColor": "rgba(0, 0, 0, 1)",
    "vim.searchHighlightColor": "rgba(255, 255, 255, 0.8)",
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.textColor": "rgba(255, 255, 255, 0.8)",
    "latex-workshop.view.pdf.viewer": "tab",
    "[latex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop"
    },
    "latex-workshop.latexindent.args": [
        "-c",
        "%DIR%/",
        "-y=defaultIndent: '%INDENT%'",
        "%TMPFILE%",
        "-m",
        "-r",
        "-l=/home/zekrom/scripts/UserSettings.yaml"
    ],
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.view.pdf.tab.editorGroup": "current",
    "latex-workshop.latex.clean.subfolder.enabled": true,
    "latex-workshop.view.pdf.backgroundColor": "#262335",
    "latex-workshop.intellisense.file.exclude": [
        "**/*.aux",
        "**/*.bbl",
        "**/*.bcf",
        "**/*.blg",
        "**/*.idx",
        "**/*.ind",
        "**/*.lof",
        "**/*.lot",
        "**/*.out",
        "**/*.toc",
        "**/*.acn",
        "**/*.acr",
        "**/*.alg",
        "**/*.glg",
        "**/*.glo",
        "**/*.gls",
        "**/*.ist",
        "**/*.fls",
        "**/*.log",
        "**/*.nav",
        "**/*.snm",
        "**/*.fdb_latexmk",
        "**/*.run.xml",
        "**/*synctex.gz"
    ],
    "latex-workshop.synctex.afterBuild.enabled": true,
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "python.experiments.enabled": false,
    "files.associations": {
        "*.rmd": "markdown"
    },
    "r.editor.tabSize": 4,
    "editor.colorDecorators": false,
    "workbench.colorTheme": "GitHub Light",
    "r.dependencyChecks": false,
    "r.interpreterPath": "/usr/bin/R",
    "editor.rulers": [
        80
    ],
    "python.dataScience.alwaysTrustNotebooks": true,
    "python.condaPath": "/opt/miniconda3/bin/conda",
    "terminal.integrated.inheritEnv": false,
    "vim.disableExtension": true
}
